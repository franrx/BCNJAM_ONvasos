<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@drawable/background" >

	<RelativeLayout
        android:id="@+id/relativeLayout1"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content">

        <ImageView
            android:id="@+id/back"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_alignParentLeft="true"
            android:contentDescription="@string/image"
            android:padding="5dp"
            android:src="@drawable/preview" />

        <ImageView
            android:id="@+id/reset"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_alignParentRight="true"
            android:contentDescription="@string/image"
            android:padding="5dp"
            android:src="@drawable/reset" />
        
    </RelativeLayout>

    <RelativeLayout
        android:paddingTop="20dp"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content" >

        <ImageView
            android:id="@+id/level1"
            android:layout_width="100dp"
            android:layout_height="120dp"
            android:layout_alignParentLeft="true"
            android:contentDescription="@string/image"
            android:padding="5dp"
            android:src="@drawable/un_desbloquejat" />

        <ImageView
            android:id="@+id/level2"
            android:layout_width="100dp"
            android:layout_height="120dp"
            android:layout_toRightOf="@+id/level1"
            android:contentDescription="@string/image"
            android:padding="5dp"
            android:src="@drawable/dos_desbloquejat" />

        <ImageView
            android:id="@+id/level3"
            android:layout_width="100dp"
            android:layout_height="120dp"
            android:layout_toRightOf="@+id/level2"
            android:contentDescription="@string/image"
            android:padding="5dp"
            android:src="@drawable/tres_desbloquejat" />

        <ImageView
            android:id="@+id/level4"
            android:layout_width="100dp"
            android:layout_height="120dp"
            android:layout_toRightOf="@+id/level3"
            android:contentDescription="@string/image"
            android:padding="5dp"
            android:src="@drawable/nivell_bloquejat" />

        <ImageView
            android:id="@+id/level5"
            android:layout_width="100dp"
            android:layout_height="120dp"
            android:layout_below="@+id/level1"
            android:layout_alignParentLeft="true"
            android:contentDescription="@string/image"
            android:padding="5dp"
            android:src="@drawable/nivell_bloquejat" />

        <ImageView
            android:id="@+id/level6"
            android:layout_width="100dp"
            android:layout_height="120dp"
            android:layout_below="@+id/level1"
            android:layout_toRightOf="@+id/level5"
            android:contentDescription="@string/image"
            android:padding="5dp"
            android:src="@drawable/nivell_bloquejat" />

        <ImageView
            android:id="@+id/level7"
            android:layout_width="100dp"
            android:layout_height="120dp"
            android:layout_below="@+id/level1"
            android:layout_toRightOf="@+id/level6"
            android:contentDescription="@string/image"
            android:padding="5dp"
            android:src="@drawable/nivell_bloquejat" />

        <ImageView
            android:id="@+id/level8"
            android:layout_width="100dp"
            android:layout_height="120dp"
            android:layout_below="@+id/level1"
            android:layout_toRightOf="@+id/level7"
            android:contentDescription="@string/image"
            android:padding="5dp"
            android:src="@drawable/nivell_bloquejat" />

        <ImageView
            android:id="@+id/level9"
            android:layout_width="100dp"
            android:layout_height="120dp"
            android:layout_below="@+id/level5"
            android:layout_alignParentLeft="true"
            android:contentDescription="@string/image"
            android:padding="5dp"
            android:src="@drawable/nivell_bloquejat" />

        <ImageView
            android:id="@+id/level10"
            android:layout_width="100dp"
            android:layout_height="120dp"
            android:layout_below="@+id/level5"
            android:layout_toRightOf="@+id/level9"
            android:contentDescription="@string/image"
            android:padding="5dp"
            android:src="@drawable/nivell_bloquejat" />

        <ImageView
            android:id="@+id/level11"
            android:layout_width="100dp"
            android:layout_height="120dp"
            android:layout_below="@+id/level5"
            android:layout_toRightOf="@+id/level10"
            android:contentDescription="@string/image"
            android:padding="5dp"
            android:src="@drawable/nivell_bloquejat" />

        <ImageView
            android:id="@+id/level12"
            android:layout_width="100dp"
            android:layout_height="120dp"
            android:layout_below="@+id/level5"
            android:layout_toRightOf="@+id/level11"
            android:contentDescription="@string/image"
            android:padding="5dp"
            android:src="@drawable/nivell_bloquejat" />

        <ImageView
            android:id="@+id/level13"
            android:layout_width="100dp"
            android:layout_height="120dp"
            android:layout_below="@+id/level9"
            android:layout_alignParentLeft="true"
            android:contentDescription="@string/image"
            android:padding="5dp"
            android:src="@drawable/nivell_bloquejat" />

        <ImageView
            android:id="@+id/level14"
            android:layout_width="100dp"
            android:layout_height="120dp"
            android:layout_below="@+id/level9"
            android:layout_toRightOf="@+id/level13"
            android:contentDescription="@string/image"
            android:padding="5dp"
            android:src="@drawable/nivell_bloquejat" />

        <ImageView
            android:id="@+id/level15"
            android:layout_width="100dp"
            android:layout_height="120dp"
            android:layout_below="@+id/level9"
            android:layout_toRightOf="@+id/level14"
            android:contentDescription="@string/image"
            android:padding="5dp"
            android:src="@drawable/nivell_bloquejat" />

        <ImageView
            android:id="@+id/level16"
            android:layout_width="100dp"
            android:layout_height="120dp"
            android:layout_alignBottom="@+id/level15"
            android:layout_toRightOf="@+id/level15"
            android:contentDescription="@string/image"
            android:padding="5dp"
            android:src="@drawable/nivell_bloquejat" />

    </RelativeLayout>
	
</LinearLayout>